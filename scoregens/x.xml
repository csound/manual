<?xml version="1.0" encoding="UTF-8"?>
<refentry xmlns="http://docbook.org/ns/docbook" version="5.0" id="x">
      <indexterm id="IndexXStatement"><primary>x statement</primary></indexterm>
  <refmeta>
    <refentrytitle>x Statement</refentrytitle>
  </refmeta>
 
  <refnamediv>
    <refname>x</refname>
    <refpurpose>
      Skip the rest of the current section.
    </refpurpose>
  </refnamediv>
 
  <refsect1>
    <title>Description</title>
    <para>
      This statement may be used to skip the rest of the current section.
    </para>
  </refsect1>
 
  <refsect1>
    <title>Syntax</title>
    <synopsis><command>x</command> anything</synopsis>
  </refsect1>
 
  <refsect1>
    <title>Initialization</title>
    <para>
      All pfields are ignored.
    </para>
  </refsect1>

  <refsect1>
    <title>Examples</title>
    <para>
      Here is an example of the x statement. It uses the file <ulink url="examples/x.csd"><citetitle>x.csd</citetitle></ulink>.
      <example>
        <title>Example of the x statement.</title>
        <xi:include xmlns:xi="http://www.w3.org/2001/XInclude" href="../examples-xml/x.csd.xml"/>
      </example>
    </para>
  </refsect1>
 
</refentry>
