  <refnamediv>
    <refname>rewindscore</refname>
    <refpurpose>
      Rewinds the playback position of the current score performance.
    </refpurpose>
  </refnamediv>

  <refsect1>
    <title>Description</title>
    <para>
     Rewinds the playback position of the current score performance..
    </para>
  </refsect1>

  <refsect1>
    <title>Syntax</title>
    <synopsis> <command>rewindscore</command></synopsis>
  </refsect1>

